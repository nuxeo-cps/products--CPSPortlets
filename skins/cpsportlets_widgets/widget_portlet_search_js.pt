function checkEmptySearch(formElem) {
    var query = trim(formElem.SearchableText.value);
    if (query != '') {
        formElem.SearchableText.value = query;
        return true;
    }
    formElem.SearchableText.value = query;
    formElem.SearchableText.focus();
    return false;
}

function trim(s) {
    if (s) {
        return s.replace(/^\s*|\s*$/g, "");
    }
    return "";
}
